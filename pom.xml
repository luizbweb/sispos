	<groupId>org.lema</groupId>
	<artifactId>sistema-pos</artifactId>
	<packaging>war</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>sistema-pos Maven Webapp</name>
	<url>http://maven.apache.org</url>
	
	<properties>
